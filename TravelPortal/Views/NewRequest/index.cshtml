                    <label class="form-check-label" for="radioNonEmployee">Non Employee</label>
                </div>
            </div>*@
        </div>

        <!-- Row 2 -->
        <!-- Row 5 -->
        <div class="row" id="stage2-row5">
            <div class="col-md-4">
                <h6>Purpose: <span class="mandatory-star">*</span></h6>
                <select class="form-select mandatoryFields" id="selectPurpose">
                    
                </select>
            </div>
            <div class="col-md-4">
                <h6>Travel Days:</h6>
                <input type="number" class="form-control" id="travelDays" placeholder="Travel Days">
            </div>

              <div class="col-md-4">
                <h6>Advance Amount:</h6>
                <input type="number" class="form-control" id="advanceAmount" placeholder="Advance Amount">
            </div>
        </div>

         <div class="row" id="rowForPassport" style="display:none">
             <div class="col-md-3">
                <h6>Name on Passport:</h6>
                <input type="text" class="form-control mandatoryFields BlockSpecialCharacters" id="NameonPassport" placeholder="Name on Passport">
            </div>
             <div class="col-md-3">
                <h6>Passport Number:</h6>
                <input type="text" class="form-control mandatoryFields BlockSpecialCharacters" id="PassportNumber" placeholder="Passport Number">
            </div>
            <div class="col-md-3 clanederIconClickRow">
                <h6>Passport Issue Date:</h6>
                <input type="text" class="form-control mandatoryFields" id="PassportIssueDate" placeholder="Passport Issue Date">
                 <div class="input-group-appendNew">
                    <span class="input-group-text">
                        <i class="fas fa-calendar clanederIconClick"></i> <!-- Font Awesome calendar icon -->
                    </span>
                </div>
            </div>
            <div class="col-md-3 clanederIconClickRow">
                <h6>Passport Expiry Date:</h6>
                <input type="text" class="form-control mandatoryFields" id="PassportExpiryDate" placeholder="Passport Expiry Date">
                <div class="input-group-appendNew">
                    <span class="input-group-text">
                        <i class="fas fa-calendar clanederIconClick"></i> <!-- Font Awesome calendar icon -->
                    </span>
                </div>
            </div>
        </div>

         <div class="row" id="rowForNominee" style="display:none">
             <div class="col-md-3">
                <h6>Name of Nominee:</h6>
                <input type="text" class="form-control mandatoryFields BlockSpecialCharacters" id="NameofNominee" placeholder="Name of Nominee">
            </div>
             <div class="col-md-3">
                <h6>Relation with Nominee:</h6>
                <input type="text" class="form-control mandatoryFields BlockSpecialCharacters" id="RelationwithNominee" placeholder="Relation with Nominee">
            </div>
            <div class="col-md-3 clanederIconClickRow">
                <h6>DOB of Nominee</h6>
                <input type="text" class="form-control mandatoryFields" id="DOBofNominee" placeholder="DOB of Nominee">
                 <div class="input-group-appendNew">
                    <span class="input-group-text">
                        <i class="fas fa-calendar clanederIconClick"></i> <!-- Font Awesome calendar icon -->
                    </span>
                </div>
            </div>

            <div class="col-md-3 clanederIconClickRow">
                <h6>Gender of Nominee:</h6>
                <select class="form-select mandatoryFields" id="selectGenderofNominee">
                    <option value="">Select Gender</option>
                    <option value="1">Male</option>
                    <option value="2">Female</option>
                    <option value="3">Other</option>
                </select>

            </div>

        </div>

        <!-- Row 6 -->
        <!-- Row 7 -->
        <div class="row" id="stage2-row7">
          
           
            <div class="col-md-8">
                <h6>Project: <span class="mandatory-star">*</span></h6>
                <select class="form-select mandatoryFields" id="selectProjectCode">
                   @* <option value="">Select Project Code</option>*@
                
                    <!-- Add more project code options here -->
                </select>
            </div>

            <div class="col-md-4">
                <h6>Currency:</h6>
                <select class="form-select mandatoryFields" id="selectCurrency">
                    
                    <!-- Add more currency options here -->
                </select>
            </div>
        </div>

         <!-- Row 8 -->
        <div class="row" id="stage2-row8">
            <div class="col-md-12">
                <h6>Comments:</h6>
                <textarea class="form-control" id="comments" rows="2" placeholder="Comments"></textarea>
            </div>
        </div>
    </div>

    

    <div class="row mt-4">
        <div class="col-md-12 text-center">
            @*<button type="button" class="btn btn-primary mt-3" style="display:none" id="previousButton">
                <span aria-hidden="true" class="mr-2">&laquo;</span> <!-- Bootstrap "Previous" icon -->
            </button>
            <button type="button" class="btn btn-primary mt-3" id="previousButtonStage2" style="display:none" 
            onclick="PreviousButtonStage2()">
                <span aria-hidden="true" class="mr-2">&laquo;</span> <!-- Bootstrap "Previous" icon -->
            </button>
            <button type="button" class="btn btn-primary mt-3" id="nextButton" onclick="ClickNextStage1()">
